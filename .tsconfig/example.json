{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "project",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "tsBuildInfoFile": "../node_modules/.tmp/tsconfig.project.tsbuildinfo",
    "outDir": "../dist/example",
    "baseUrl": "../src/example/",
    "rootDir": "../src/example",
    // The 'import.meta' meta-property is only allowed when the '--module'
    // option is 'es2020', 'es2022', 'esnext', 'system', 'node16', or 'nodenext'
    "module": "Node16",
    "jsx": "react"
  },
  "include": [
    "../src/example"
  ]
}
